{
    "exclude": ["node_modules"],
    "compilerOptions": {
        "module": "Node16",
        "baseUrl": "./",
        "paths": {
            "#Lib/*" : ["./src/lib/*"],
            "#Config/*": ["./src/config/*"],
            "#Routes/*": ["./src/routes/*"],
            "#Controllers/*": ["./src/controller/*"],
            "#Schemas/*": ["./src/schemas/*"],
            "#Dto/*": ["./src/dto/*"]
        }
    }
}
